package lambdas;

public class Thread {

	public static void main(String[] args) {
		
		Runnable trabalho1 = new Trabalho1();
		Runnable trabalho2 = new Runnable() {
			public void run() {
				for(int i = 0; i < 100; i++) {
					System.out.println("Tarefa#1");
				}
			}
		};
		
		Thread t1 = new Thread(tarefa1);
		Thread t2 = new Thread(tarefa2);
		
	}
}
